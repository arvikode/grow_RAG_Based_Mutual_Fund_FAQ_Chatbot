# API Keys for LLM Integration
# Copy this file to .env and replace with your actual API keys

# Google Gemini API Key (Default LLM Provider)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Grok API Key (Alternative LLM Provider)  
# Get your key from: https://console.x.ai/
GROK_API_KEY=your_grok_api_key_here

# LLM Provider Selection (gemini or grok)
LLM_PROVIDER=gemini

# Model Configuration
GEMINI_MODEL=gemini-1.5-flash
GROK_MODEL=grok-beta

# Response Settings
MAX_TOKENS=1000
TEMPERATURE=0.1
